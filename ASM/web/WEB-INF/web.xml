        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>keebServlet</servlet-name>
        <url-pattern>/keeb</url-pattern>
    </servlet-mapping>
