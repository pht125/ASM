        <servlet-name>mouseServlet</servlet-name>
        <url-pattern>/mouse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>padServlet</servlet-name>
        <url-pattern>/pad</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>switchServlet</servlet-name>
        <url-pattern>/switch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpServlet</servlet-name>
